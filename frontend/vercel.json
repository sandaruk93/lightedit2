{
  "version": 2,
  "buildCommand": "GENERATE_SOURCEMAP=false CI=false npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://lightedit-backend.onrender.com/:path*"
    }
  ]
} 